{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "arrow-parens": "off",
    "no-unused-vars": "off",
    "import/no-unresolved": "off",
    "no-undef": "off",
    "class-methods-use-this": "off"
  }
}
